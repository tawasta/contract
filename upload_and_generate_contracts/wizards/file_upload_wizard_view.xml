<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_file_upload_wizard_form" model="ir.ui.view">
        <field name="name">file.upload.wizard.form</field>
        <field name="model">file.upload.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="file_data" filename="file_name"/>
                </group>
                <footer>
                    <button string="Upload" type="object" name="create_partner_and_contract" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
